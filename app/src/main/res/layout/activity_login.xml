<?xml version="1.0" encoding="utf-8"?>
<AbsoluteLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.fmoya.tuhoroscopo.Login"
    tools:ignore="Deprecated">


<TextView
android:id="@+id/textView3"
android:layout_width="316dp"
android:layout_height="wrap_content"
android:layout_x="38dp"
android:layout_y="308dp"
android:text="@string/saludo"
android:textAppearance="@style/TextAppearance.AppCompat.Body2"
android:textSize="18sp" />

    <ImageButton
android:id="@+id/presiona"
style="@android:style/Widget.ImageButton"
android:layout_width="205dp"
android:layout_height="202dp"
android:layout_x="85dp"
android:layout_y="94dp"
android:adjustViewBounds="true"
android:cropToPadding="true"
android:visibility="visible"
app:srcCompat="@mipmap/ic_launcher"
android:contentDescription=""
tools:ignore="ContentDescription" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_x="60dp"
        android:layout_y="31dp"
        android:text="@string/hor_scopo_chino"
        android:textAppearance="@style/TextAppearance.AppCompat.Display1"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.178" />

    <EditText
        android:id="@+id/usu"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_x="95dp"
        android:layout_y="381dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="" />

    <EditText
        android:id="@+id/pas"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_x="95dp"
        android:layout_y="436dp"
        android:ems="10"
        android:inputType="textPassword"
        android:text="" />



</AbsoluteLayout>

